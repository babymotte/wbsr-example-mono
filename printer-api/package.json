{
  "name": "@babymotte/printer-api",
  "version": "1.0.0",
  "author": {
    "name": "Michael Bachmann",
    "email": "mbachmann@bbmsoft.net"
  },
  "license": "MIT",
  "moduleType": "API",
  "dataTypes": [
    {
      "name": "PrintJob",
      "struct": {
        "properties": {
          "name": {
            "type": "string"
          },
          "mode": {
            "enum": [
              "STD",
              "ERR"
            ]
          },
          "text": {
            "type": "string"
          }
        }
      }
    }
  ],
  "interfaces": [
    {
      "name": "Printer",
      "functions": [
        {
          "name": "print",
          "arguments": [
            {
              "name": "job",
              "type": {
                "import": {
                  "name": "PrintJob"
                }
              }
            }
          ]
        }
      ]
    }
  ]
}
